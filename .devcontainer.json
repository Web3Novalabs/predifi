{
  "name": "dev",
  "image": "mcr.microsoft.com/devcontainers/base:debian",
  "customizations": {
    "vscode": {
      "extensions": [
        "StarkWare.cairo1",
        "tamasfe.even-better-toml",
        "rust-lang.rust-analyzer"
      ]
    }
  },
  "features": {
    "ghcr.io/devcontainers/features/rust:1": {
      "version": "stable"
    }
  },
  "postCreateCommand": "bash .devcontainer/install-tools.sh",
  "remoteUser": "vscode"
}
